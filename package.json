{
  "name": "code-thing",
  "version": "1.0.0",
  "description": "A thing that does things on some text input",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build-and-watch": "cross-env NODE_ENV=production webpack --mode development --watch",
    "compile-standalone": "npm run build && pkg --compress Brotli build/index.js -c package.json --out-path build/",
    "compile-standalone-without-compression": "npm run build && pkg build/index.js -c package.json --out-path build/",
    "run-script": "cross-env NODE_ENV=production node build/index.js",
    "start": "npm run build && npm run run-script"
  },
  "author": "vladimir.tasik@gmail.com",
  "license": "ISC",
  "pkg": {
    "assets": "build/maps/*"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/jest": "27.5.1",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "nodemon": "2.0.16",
    "pkg": "5.7.0",
    "ts-jest": "28.0.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.4",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "inquirer": "8.2.4",
    "winston": "3.7.2"
  }
}
